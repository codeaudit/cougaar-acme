# Rule to insert NAIJaasBinder to all node agents
# plugin = org.cougaar.core.security.access.JaasAgentBinderFactory 

plugin = "org.cougaar.core.security.access.JaasAgentBinderFactory"

society.each_node_agent() do |node_agent|
   node_agent.add_component do |c|
      c.classname = plugin
   end
end
